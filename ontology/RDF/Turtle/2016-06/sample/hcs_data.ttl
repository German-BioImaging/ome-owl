pixels:pixel0  a  ome:Pixels .

bindata:bindata0  a    ome:BinData ;
        ome:bigEndian  false ;
        ome:length     "0"^^xsd:nonNegativeLong .

_:b0    a          PATO:0001242 ;
        rdf:value  "488.0"^^xsd:float ;
        qudt:unit  unitLength:nm .

_:b1    a          PATO:0000117 ;
        rdf:value  "0.207"^^xsd:float ;
        qudt:unit  unitLength:um .

image:image0  a              ome:Image ;
