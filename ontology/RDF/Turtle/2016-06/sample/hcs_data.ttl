        rdfs:label           "Series 1"@en ;
        dcterms:identifier   "Image:0" ;
        ome:acquisitionDate  "2008-02-06T13:43:19" ;
        ome:pixels           pixels:pixel0 .

channel:channel2  a               ome:Channel ;
        rdfs:label                "TD1"@en ;
        dcterms:identifier        "Channel:0:2" ;
        ome:acquisitionMode       acquisitionMode:laserScanningConfocalMicroscopy ;
        ome:contrastMethod        contrastMethod:dic ;
