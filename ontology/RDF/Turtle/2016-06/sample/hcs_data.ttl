        rdfs:label                "CH1"@en ;
        dcterms:identifier        "Channel:0:0" ;
        ome:acquisitionMode       acquisitionMode:laserScanningConfocalMicroscopy ;
        ome:emissionWavelength    [ a          PATO:0001242 ;
                                    rdf:value  "523.0"^^xsd:float ;
                                    qudt:unit  unitLength:nm
                                  ] ;
        ome:excitationWavelength  _:b0 ;
        ome:illuminationType      illuminationType:epiflorescence ;
