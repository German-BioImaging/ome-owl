        rdf:value           map:map2.2 , map:map2.1 ;
        rdfs:comment        "This is the description of the sample map B"@en ;
        dcterms:identifier  "Annotation:2" .

pixelType:uint8  a  ome:PixelType .

bindata:bindata0  a    ome:BinData ;
        ome:bigEndian  false ;
        ome:data       "/wCrzur//wB5oMPi/wBIbJO3AP8ePGCF"^^xsd:base64Binary ;
        ome:length     "32"^^xsd:nonNegativeLong .

[ a               ome:StructuredAnnotations ;
  ome:annotation  mapAnnotation:mapAnnotation2 , mapAnnotation:mapAnnotation1
] .

map:map2.1  a      ome:Map ;
        ome:key    "SampleKeyB-1" ;
        ome:value  "SampleValueB-1" .
