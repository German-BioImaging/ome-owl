        rdfs:comment        "This is the description of the sample map A"@en ;
        dcterms:identifier  "Annotation:1" .

map:map2.2  a      ome:Map ;
        ome:key    "SampleKeyB-2" ;
        ome:value  "SampleValueB-2" .

image:image0  a              ome:Image ;
        rdfs:label           "6x6x1x8-swatch.tif"@en ;
        dcterms:identifier   "Image:0" ;
        ome:acquisitionDate  "2010-02-23T12:51:30" ;
        ome:annotation       mapAnnotation:mapAnnotation2 , mapAnnotation:mapAnnotation1 ;
        ome:pixels           pixels:pixels0.0 .

pixels:pixels0.0  a         ome:Pixels ;
        dcterms:identifier  "Pixels:0.0" ;
        ome:binData         bindata:bindata0 ;
        ome:dimensionOrder  dimensionOrder:xyczt ;
        ome:physicalSizeX   _:b0 ;
        ome:physicalSizeY   _:b0 ;
        ome:pixcelType      pixelType:uint8 ;
        ome:sizeC           "1"^^xsd:positiveInteger ;
        ome:sizeT           "1"^^xsd:positiveInteger ;
        ome:sizeX           "6"^^xsd:positiveInteger ;
        ome:sizeY           "4"^^xsd:positiveInteger ;
        ome:sizeZ           "1"^^xsd:positiveInteger .

_:b0    a          PATO:0000117 ;
        rdf:value  "100000.0"^^xsd:float ;
        qudt:unit  unitLength:nm .

map:map1  a        ome:Map ;
        ome:key    "SampleKeyA" ;
        ome:value  "SampleValueA" .

dimensionOrder:xyczt  a  ome:DimensionOrder .

mapAnnotation:mapAnnotation2
